<register_page>
  <registers>
  
    <register execution_state="AArch64" is_register="True" is_internal="True" is_stub_entry="False">
      <reg_short_name>FGWTE3_EL3</reg_short_name>
        
        <reg_long_name>Fine-Grained Write Traps EL3</reg_long_name>



      
            <reg_condition otherwise="UNDEFINED">when EL3 is implemented, FEAT_FGWTE3 is implemented, and FEAT_AA64 is implemented</reg_condition>
      




          <reg_reset_value></reg_reset_value>

      <reg_mappings>
        




      </reg_mappings>

        <reg_purpose>
          
    
      <purpose_text>
        <para>Provides controls for traps of <instruction>MSR</instruction> and <instruction>MSRR</instruction> writes to specified EL3 system registers.</para>
      </purpose_text>

        </reg_purpose>

      <reg_groups>
          <reg_group>Unknown</reg_group>
      </reg_groups>
      
      
        
      <reg_attributes>
          
    
      <attributes_text>
        <para>FGWTE3_EL3 is a 64-bit register.</para>
      </attributes_text>

      </reg_attributes>
      <reg_fieldsets>
        





<fields id="fieldset_0" length="64">
  <text_before_fields><para><instruction>MSR</instruction> accesses are trapped to EL3 and reported with EC syndrome value <hexnumber>0x18</hexnumber>.</para>
<para><instruction>MSRR</instruction> accesses are trapped to EL3 and reported with EC syndrome value <hexnumber>0x14</hexnumber>.</para>
<para>The bits in this register are sticky. Writes to these bits have the following properties:</para>
<list type="unordered">
<listitem><content>A write of <binarynumber>0b0</binarynumber> is ignored.</content>
</listitem><listitem><content>A write of <binarynumber>0b1</binarynumber> updates the bit to <binarynumber>0b1</binarynumber>.</content>
</listitem></list></text_before_fields>
